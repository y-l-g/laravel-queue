{
  "name": "pogo/laravel-queue",
  "description": "Pogo Queue Driver for Laravel",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.4",
    "laravel/framework": "^10.0 || ^11.0 || ^12.0",
    "laravel/octane": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Pogo\\Queue\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pogo\\Queue\\Laravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pogo\\Queue\\Laravel\\PogoQueueServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
